
@{
    ViewBag.Title = "EgitimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<!DOCTYPE html>
<html>
<body class="hold-transition sidebar-mini">
    <section class="content">
        <div class="container-fluid">
            <div class="row">

                <!-- /.col -->
                <div class="col-md-12 p-2">
                    <div class="card">
                        <div class="card-header">
                            <h3 class="card-title">Yeni Eğitim Ekleme Sayfası</h3>
                        </div>
                        <div class="card-body">

                            <form method="post">

                                <input type="text" name="Baslik" class="form-control" placeholder="Başlığı yazınız" />
                                <br />
                                <input type="text" name="AltBaslik1" class="form-control" placeholder="Alt Başlığı yazınız" />
                                <br />
                                <input type="text" name="AltBaslik2" class="form-control" placeholder="Alt Başlığı yazınız" />
                                <br />
                                <input type="text" name="GNO" class="form-control" placeholder="GNO yazınız" />
                                <br />

                                <input type="text" name="Tarih" class="form-control" placeholder="Eğitim Tarihi" />
                                <br />
                                <button class="btn btn-info">Kaydet</button>
                            </form>
                        </div>
                    </div>


                </div>
            </div>

        </div>


    </section>
</body>
</html>